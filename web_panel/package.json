{
  "name": "mumudvb-web-panel",
  "version": "2.0.0",
  "description": "Advanced web interface for MuMuDVB DVB streaming server with Ubuntu support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-service": "sudo cp ../scripts/mumudvb-web-panel.service /etc/systemd/system/ && sudo systemctl daemon-reload",
    "enable-service": "sudo systemctl enable mumudvb-web-panel",
    "start-service": "sudo systemctl start mumudvb-web-panel",
    "stop-service": "sudo systemctl stop mumudvb-web-panel",
    "status-service": "sudo systemctl status mumudvb-web-panel",
    "setup": "npm install && echo Setup completed successfully"
  },
  "keywords": [
    "mumudvb",
    "dvb",
    "dvb-s",
    "dvb-t", 
    "dvb-c",
    "streaming",
    "web",
    "panel",
    "ubuntu",
    "linux",
    "oscam",
    "descrambling"
  ],
  "author": "GitHub Copilot",
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/braice/MuMuDVB.git"
  },
  "bugs": {
    "url": "https://github.com/braice/MuMuDVB/issues"
  },
  "homepage": "https://github.com/braice/MuMuDVB#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.13.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "os": [
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}